        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "autoprefixer": "^9.4.5",
        "babel-eslint": "^9.0.0",
        "postcss-cli": "^6.1.1",
        "tailwindcss": "^0.7.3"
    }
}